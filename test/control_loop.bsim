Ts = 1e-2 ;
Duration = 4 ;

nets = {net0, net1, net2, net3, net4}

blocks = {step1, add1, tfs1, tfs2, gain1}

scopes = {net0, net1, net2, net3, net4}

## nets

net0 = {  Input : step1 ;
          Output : add1[0] ;
          name : "net0"
}

net1 = {  Input : add1[2] ;
          Output : gain1 ;
          name : "net1"
}

net2 = {  Input : gain1 ;
          Output : tfs1  ;
          name : "net2"
}

net3 = {  Input : tfs1 ;
          Output : tfs2 ;
          name : "net3"
}

net4 = {  Input : tfs2  ;
          Output : add1[1]  ;
          name : "net4"
}


## blocks

step1 = { type : step ;
          value : 1 ; 
          time : 1           
}

add1 = { type : add ;
         inputs : 2 ;
         value : p, n
}


tfs1 = { type : tfs ;
         num : 1 ;
         den : 1,1 
}

tfs2 = { type : tfs ;
         num : 10 ;
         den : 1,10 
}

gain1 = {   type : gain;
            value : 5
}


